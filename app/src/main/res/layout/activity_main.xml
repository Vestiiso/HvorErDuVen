<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">



    <Button
        android:id="@+id/testHovedsiden"
        android:layout_width="214dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:text="Test Hovedsiden"
        android:textStyle="bold" />

    <Button
        android:id="@+id/testSendSMS"
        android:layout_width="214dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:text="test SendSMS" />

    <Button
        android:id="@+id/testSettings"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="test Settings" />

    <Button
        android:id="@+id/testLogin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="test Log Ind" />


</LinearLayout>

